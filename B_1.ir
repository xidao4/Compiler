FUNCTION mod :
PARAM x
PARAM n
t4 := x / n
t3 := t4 * n
t1 := x - t3
RETURN t1
FUNCTION DigitSum :
PARAM y
IF y == #0 GOTO label1
GOTO label2
LABEL label1 :
RETURN #0
LABEL label2 :
ARG #10
ARG y
t12 := CALL mod
t16 := y / #10
ARG t16
t13 := CALL DigitSum
t11 := t12 + t13
RETURN t11
FUNCTION main :
READ t19
num := t19
IF num < #0 GOTO label3
GOTO label4
LABEL label3 :
t22 := #0 - #1
WRITE t22
GOTO label5
LABEL label4 :
ARG num
t24 := CALL DigitSum
WRITE t24
LABEL label5 :
RETURN #0
